
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="shortcut icon" href="~/Resources/favicon.ico" type="image/x-icon" />
    @*<link rel="stylesheet" href="~/Content/font-awesome.min.css">*@
    <title>Intranet-Fênix</title>
    <link  rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css">
    
    <script src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" href="~/Content/Intra.css" type="text/css">

    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.9.2.custom.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" href="~/Content/jquery-ui-1.9.2.custom.min" type="text/css">
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">

        google.charts.load('current', { 'packages': ['corechart'] });

        // Set a callback to run when the Google Visualization API is loaded.
        google.charts.setOnLoadCallback(drawChart);

        // Callback that creates and populates a data table,
        // instantiates the pie chart, passes in the data and
        // draws it.
        function drawChart() {

            // Create the data table.
            var data = new google.visualization.DataTable();
            data.addColumn('string', 'Placas Mãe');
            data.addColumn('number', 'Quantidades');
            data.addRows([
                ['Placa 1', 3],
                ['Placa 2', 1],
                ['Placa 3', 1],
                ['Placa 4', 1],
                ['Placa 5', 2]
            ]);

            // Set chart options
            var options = {
                'title': 'Produção',
                'width': 300,
                'height': 230
            };

            // Instantiate and draw our chart, passing in some options.
            var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
            chart.draw(data, options);
        }
       

    </script>


    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/fonts/svg")
    @Styles.Render("~/Content/Intra")
    @Styles.Render("~/Content/Menu")


</head>
<body >
    <div class="layout">
        <header class="cabecalho">

            <div class="row fundo-nav">

                <div class="row over-fundo-nav">

                    <div class="col-md-6 col-sm-6 col-lg-6 ">

                        <div class="div-logo">

                            <img id="logo-intra" src="~/Resources/Logo-Negativada.png" alt="imagem11">

                        </div>



                    </div>


                    <div class="col-md-6  col-sm-6 col-lg-6 ">
                        <div class="container">

                            <div id="div-login" class="container">

                                <span style="color:wheat;"> @Recursos.Resources.BemVindo @ViewBag.UsuarioLogado !</span>

                                <a href="~/Home/LogOff">@Recursos.Resources.Sair</a>

                                <ul id="ul-links" class="nav ">
                                    <li class="item-141 toggle-contraste"><a class="toggle-contraste" href="#" accesskey="6">@Recursos.Resources.ALTO @Recursos.Resources.CONTRASTE</a></li>
                                </ul>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>


        <div class="row">

            <div class="col-md-3 col-sm-3 col-lg-3">

                <div id="container-menu-dinamico" class="container">


                    

                    @Html.Action("MenuDinamico", "Menu")


                     

                </div>

            </div>


            <div class="col-md-9 col-sm-9 col-lg-9">

                <div id="centro" class="container">

                    @RenderBody()
                    <br>


                </div>


            </div>




        </div>

        <div class="row">
            <div class="footer">
                <footer class="bloco-footer">

                    <div>

                        <br>

                    </div>


                </footer>
            </div>
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/Intranet")
        @Scripts.Render("~/bundles/NTM")


        @RenderSection("scripts", required: false)
    </div>
</body>
</html>
